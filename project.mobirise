          "_name": "tabs1",
          "_customHTML": "<section class=\"tabs1\" group=\"Tabs\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"color\" title=\"Tabs Color\" name=\"tabsColor\" value=\"#efefef\">\n        <input type=\"color\" title=\"Active Tab Color\" name=\"activeTabColor\" value=\"#977a43\">\n        <input type=\"color\" title=\"Tabs Bg\" name=\"tabsBg\" value=\"#f6f6f6\">\n        <select title=\"Tabs Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected>5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"range\" title=\"Tab 1 Items\" inline name=\"items1\" min=\"1\" max=\"50\" step=\"1\" value=\"3\">\n        <input type=\"range\" title=\"Tab 2 Items\" inline name=\"items2\" min=\"1\" max=\"50\" step=\"1\" value=\"3\" condition=\"bootstrapTabs>1\">\n        <input type=\"range\" title=\"Tab 3 Items\" inline name=\"items3\" min=\"1\" max=\"50\" step=\"1\" value=\"3\" condition=\"bootstrapTabs>2\">\n        <input type=\"range\" title=\"Tab 4 Items\" inline name=\"items4\" min=\"1\" max=\"50\" step=\"1\" value=\"3\" condition=\"bootstrapTabs>3\">\n        <in